{
  "compilerOptions": {
    "incremental": true,
    "target": "es6",
    "module": "CommonJS",
    "lib": ["ES6", "ES5"],
    "sourceMap": true,
    "outDir": "./dist",
    "strict": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["."],
  
}
